[
  {
    "name": "cadvisor",
    "image": "google/cadvisor",
    "cpu": 10,
    "memory": 300,
    "portMappings": [
      {
        "containerPort": 8080,
        "hostPort": 9190
      }
    ],
    "essential": true,
    "mountPoints": [
      {
        "sourceVolume": "root",
        "containerPath": "/rootfs",
        "readOnly": true
      },
      {
        "sourceVolume": "var_run",
        "containerPath": "/var/run",
        "readOnly": false
      },
      {
        "sourceVolume": "sys",
        "containerPath": "/sys",
        "readOnly": true
      },
      {
        "sourceVolume": "cgroup",
        "containerPath": "/sys/fs/cgroup",
        "readOnly": true
      },
      {
        "sourceVolume": "var_lib_docker",
        "containerPath": "/var/lib/docker",
        "readOnly": true
      }
    ]
  }
]